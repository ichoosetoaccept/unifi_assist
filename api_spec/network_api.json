    "/proxy/network/api/s/{site}/cmd/stamgr": {
      "post": {
        "summary": "Manage client devices",
        "description": "Perform actions on client devices such as blocking/unblocking",
        "parameters": [
          {
            "name": "site",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Site identifier"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["cmd", "mac"],
                "properties": {
                  "cmd": {
                    "type": "string",
                    "enum": ["block-sta", "unblock-sta", "forget-sta"],
                    "description": "Command to execute"
                  },
                  "mac": {
                    "type": "string",
                    "description": "MAC address of the client"
                  }
                }
              },
              "examples": {
                "block_client": {
                  "summary": "Block a client",
                  "value": {
                    "cmd": "block-sta",
                    "mac": "aa:bb:cc:00:11:22"
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Command executed successfully",
            "content": {
              "application/json": {
                "example": {
                  "meta": {
                    "rc": "ok"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/proxy/network/api/s/{site}/cmd/devmgr": {
      "post": {
        "summary": "Manage network devices",
        "description": "Perform actions on network devices such as adopting, forgetting, or upgrading",
        "parameters": [
          {
            "name": "site",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Site identifier"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["cmd", "mac"],
                "properties": {
                  "cmd": {
                    "type": "string",
                    "enum": [
                      "adopt",
                      "forget",
                      "upgrade",
                      "restart",
                      "force-provision",
                      "set-locate",
                      "unset-locate",
                      "move"
                    ],
                    "description": "Command to execute"
                  },
                  "mac": {
                    "type": "string",
                    "description": "MAC address of the device"
                  },
                  "site": {
                    "type": "string",
                    "description": "Target site ID when using move command"
                  }
                }
              },
              "examples": {
                "locate_device": {
                  "summary": "Enable device location LED",
                  "value": {
                    "cmd": "set-locate",
                    "mac": "f0:9f:c2:00:00:00"
                  }
                },
                "move_device": {
                  "summary": "Move device to another site",
                  "value": {
                    "cmd": "move",
                    "mac": "f0:9f:c2:00:00:00",
                    "site": "abcdef123456789"
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Command executed successfully",
            "content": {
              "application/json": {
                "example": {
                  "meta": {
                    "rc": "ok"
                  }
                }
              }
            }
          }
        }
      }
