                              },
                              "mem": {
                                "type": "number",
                                "description": "Gateway memory usage percentage"
                              },
                              "uptime": {
                                "type": "integer",
                                "description": "Gateway uptime in seconds"
                              },
                              "temperature": {
                                "type": "number",
                                "description": "Gateway temperature in Celsius"
                              }
                            }
                          },
                          "speedtest_status": {
                            "type": "object",
                            "properties": {
                              "status": {
                                "type": "string",
