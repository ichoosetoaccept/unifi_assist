    "/proxy/network/api/s/{site}/stat/health": {
      "get": {
        "summary": "Get network health statistics",
        "description": "Returns health metrics for the network including subsystem status",
        "parameters": [
          {
            "name": "site",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Site identifier"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
