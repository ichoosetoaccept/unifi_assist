                            "type": "boolean",
                            "description": "Whether client is blocked"
                          },
                          "anomalies": {
                            "type": "integer",
                            "description": "Number of anomalies detected"
                          },
                          "stat": {
                            "type": "object",
                            "properties": {
                              "signal": {
                                "type": "integer",
                                "description": "Signal strength in dBm"
                              },
                              "noise": {
                                "type": "integer",
                                "description": "Noise floor in dBm"
                              },
                              "rx_rate": {
                                "type": "integer",
                                "description": "Receive rate in Mbps"
                              },
                              "tx_rate": {
                                "type": "integer",
                                "description": "Transmit rate in Mbps"
                              },
                              "rx_bytes": {
                                "type": "integer",
