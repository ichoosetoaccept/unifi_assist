              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "subsystem": {
                            "type": "string",
                            "description": "Subsystem name",
                            "enum": ["wan", "lan", "wlan", "vpn"]
                          },
                          "status": {
                            "type": "string",
                            "description": "Health status",
                            "enum": ["ok", "warning", "critical"]
                          },
                          "num_user": {
                            "type": "integer",
                            "description": "Number of users"
                          },
                          "num_guest": {
                            "type": "integer",
                            "description": "Number of guests"
                          },
                          "num_iot": {
                            "type": "integer",
                            "description": "Number of IoT devices"
                          },
                          "tx_bytes-r": {
                            "type": "integer",
                            "description": "Current transmit rate in bytes/sec"
                          },
                          "rx_bytes-r": {
                            "type": "integer",
                            "description": "Current receive rate in bytes/sec"
                          },
                          "status_vpn": {
                            "type": "string",
                            "enum": ["connected", "disconnected", "connecting"],
                            "description": "VPN connection status"
                          },
                          "num_disconnected": {
                            "type": "integer",
                            "description": "Number of disconnected devices"
                          },
                          "gw_system_stats": {
                            "type": "object",
                            "properties": {
                              "cpu": {
                                "type": "number",
