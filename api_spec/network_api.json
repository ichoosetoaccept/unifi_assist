                "example": {
                  "data": [
                    {
                      "subsystem": "wan",
                      "status": "ok",
                      "status_vpn": "connected",
                      "num_disconnected": 1,
                      "gw_system_stats": {
                        "cpu": 15.2,
                        "mem": 45.6,
                        "uptime": 1234567,
                        "temperature": 42.5
                      },
                      "speedtest_status": {
                        "status": "Idle",
                        "last_run": 1705609846,
                        "download": 235.5,
                        "upload": 24.8,
                        "latency": 12.3
                      }
                    }
                  ]
                }
