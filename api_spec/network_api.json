                }
              }
            }
          },
          "401": {
            "description": "Unauthorized - Invalid API key",
            "content": {
              "application/json": {
                "example": {
                  "meta": {
                    "rc": "error",
                    "msg": "api.err.InvalidAPIKey"
                  }
                }
              }
            }
          },
          "404": {
            "description": "Site not found",
            "content": {
              "application/json": {
                "example": {
                  "meta": {
                    "rc": "error",
                    "msg": "api.err.SiteNotFound"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/proxy/network/api/s/{site}/stat/sta": {
      "get": {
        "summary": "Get client statistics",
        "description": "Returns statistics for all connected clients in a site",
        "parameters": [
          {
            "name": "site",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Site identifier"
          },
          {
            "name": "type",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["all", "active", "disconnected"]
            },
            "description": "Filter by client connection status"
          }
        ],
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "mac": {
                            "type": "string",
                            "description": "Client MAC address"
                          },
                          "ip": {
                            "type": "string",
                            "description": "Client IP address"
                          },
                          "hostname": {
                            "type": "string",
                            "description": "Client hostname"
                          },
                          "name": {
                            "type": "string",
                            "description": "Client name (if set)"
                          },
                          "first_seen": {
                            "type": "integer",
                            "description": "First seen timestamp"
                          },
                          "last_seen": {
                            "type": "integer",
                            "description": "Last seen timestamp"
                          },
                          "is_wired": {
                            "type": "boolean",
                            "description": "Whether client is connected via wire"
                          },
                          "is_guest": {
                            "type": "boolean",
                            "description": "Whether client is on guest network"
                          },
                          "network": {
                            "type": "string",
                            "description": "Network name"
                          },
                          "network_id": {
                            "type": "string",
                            "description": "Network identifier"
                          },
                          "oui": {
                            "type": "string",
                            "description": "Manufacturer OUI"
                          },
                          "radio": {
                            "type": "string",
                            "description": "Radio band (na or ng)",
