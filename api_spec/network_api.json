                              "last_run": {
                                "type": "integer",
                                "description": "Last speedtest timestamp"
                              },
                              "download": {
                                "type": "number",
                                "description": "Download speed in Mbps"
                              },
                              "upload": {
                                "type": "number",
                                "description": "Upload speed in Mbps"
                              },
                              "latency": {
                                "type": "number",
                                "description": "Latency in milliseconds"
                              }
                            }
                          }
