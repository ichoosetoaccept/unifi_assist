{
  "openapi": "3.0.0",
  "info": {
    "title": "UniFi Network API",
    "version": "9.0.99",
    "description": "API for interacting with UniFi Network Application"
  },
  "servers": [
    {
      "url": "https://{controller}",
      "variables": {
        "controller": {
          "description": "UniFi Network Application controller address",
          "default": "192.168.1.1"
        }
      }
    }
  ],
  "paths": {
    "/proxy/network/integration/v1/sites": {
      "get": {
        "summary": "List all sites",
        "description": "Returns a list of all sites available to the authenticated user",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "description": "Site identifier"
                      },
                      "name": {
                        "type": "string",
                        "description": "Site name"
                      },
                      "desc": {
                        "type": "string",
                        "description": "Site description"
                      },
                      "role": {
                        "type": "string",
                        "description": "User's role at this site",
                        "enum": ["admin", "readonly", "none"]
                      }
                    }
                  }
                },
                "example": [
                  {
                    "id": "default",
                    "name": "Default",
                    "desc": "Default site",
                    "role": "admin"
                  }
                ]
              }
            }
          },
          "401": {
            "description": "Unauthorized - Invalid API key",
            "content": {
              "application/json": {
                "example": {
                  "meta": {
                    "rc": "error",
                    "msg": "api.err.InvalidAPIKey"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/proxy/network/api/s/{site}/stat/device": {
      "get": {
        "summary": "Get device statistics",
        "description": "Returns statistics for all devices in a site",
        "parameters": [
          {
            "name": "site",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Site identifier"
          },
          {
            "name": "within",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer"
            },
            "description": "Time window in hours for the statistics"
          },
          {
            "name": "type",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["uap", "ugw", "usw", "udm", "unvr"]
            },
            "description": "Filter by device type"
          }
        ],
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "mac": {
                            "type": "string",
                            "description": "Device MAC address"
                          },
                          "ip": {
                            "type": "string",
                            "description": "Device IP address"
                          },
                          "model": {
                            "type": "string",
                            "description": "Device model"
                          },
                          "model_in_lts": {
                            "type": "boolean",
                            "description": "Whether device model is in LTS"
                          },
                          "model_in_eol": {
                            "type": "boolean",
                            "description": "Whether device model is EOL"
                          },
                          "type": {
                            "type": "string",
                            "description": "Device type",
                            "enum": ["uap", "ugw", "usw", "udm", "unvr"]
                          },
                          "version": {
                            "type": "string",
                            "description": "Firmware version"
                          },
                          "adopted": {
                            "type": "boolean",
                            "description": "Whether device is adopted"
                          },
                          "status": {
                            "type": "string",
                            "description": "Device status",
                            "enum": ["online", "offline", "pending", "upgrading", "provisioning", "adopting"]
                          },
                          "uptime": {
                            "type": "integer",
                            "description": "Device uptime in seconds"
                          },
                          "last_seen": {
                            "type": "integer",
                            "description": "Last seen timestamp"
                          },
                          "upgradable": {
                            "type": "boolean",
                            "description": "Whether firmware upgrade is available"
                          },
                          "upgrade_to_firmware": {
                            "type": "string",
                            "description": "Available firmware version"
                          },
                          "provisioned": {
                            "type": "boolean",
                            "description": "Whether device is provisioned"
                          },
                          "satisfaction": {
                            "type": "integer",
                            "description": "Device satisfaction score (0-100)",
                            "minimum": 0,
                            "maximum": 100
                          },
                          "satisfaction_reason": {
                            "type": "integer",
                            "description": "Reason code for current satisfaction score",
                            "enum": [0, 1, 2, 3, 4],
                            "x-enum-descriptions": [
                              "No issues",
                              "High memory usage",
                              "High CPU usage",
                              "Poor connectivity",
                              "Configuration issues"
                            ]
                          },
                          "name": {
                            "type": "string",
                            "description": "Device name"
                          },
                          "serial": {
                            "type": "string",
                            "description": "Device serial number"
                          },
                          "stat": {
                            "type": "object",
                            "properties": {
                              "bytes": {
                                "type": "integer",
                                "description": "Total bytes transferred"
                              },
                              "rx_bytes": {
                                "type": "integer",
                                "description": "Bytes received"
                              },
                              "tx_bytes": {
                                "type": "integer",
                                "description": "Bytes transmitted"
                              },
                              "rx_rate": {
                                "type": "integer",
                                "description": "Current receive rate in bytes/sec"
                              },
                              "tx_rate": {
                                "type": "integer",
                                "description": "Current transmit rate in bytes/sec"
                              },
                              "wifi_tx_attempts": {
                                "type": "integer",
                                "description": "WiFi transmission attempts"
                              },
                              "wifi_tx_drops": {
                                "type": "integer",
                                "description": "WiFi transmission drops"
                              }
                            }
                          },
                          "sys_stats": {
                            "type": "object",
                            "properties": {
                              "loadavg_1": {
                                "type": "number",
                                "description": "1-minute load average"
                              },
                              "loadavg_5": {
                                "type": "number",
                                "description": "5-minute load average"
                              },
                              "loadavg_15": {
                                "type": "number",
                                "description": "15-minute load average"
                              },
                              "mem_total": {
                                "type": "integer",
                                "description": "Total memory in bytes"
                              },
                              "mem_used": {
                                "type": "integer",
                                "description": "Used memory in bytes"
                              },
                              "cpu": {
                                "type": "number",
                                "description": "CPU usage percentage"
                              }
                            }
                          },
                          "config_network": {
                            "type": "object",
                            "properties": {
                              "type": {
                                "type": "string",
                                "enum": ["dhcp", "static"],
                                "description": "Network configuration type"
                              },
                              "ip": {
                                "type": "string",
                                "description": "Static IP address if configured"
                              },
                              "netmask": {
                                "type": "string",
                                "description": "Network mask"
                              },
                              "gateway": {
                                "type": "string",
                                "description": "Gateway address"
                              },
                              "dns": {
                                "type": "array",
                                "items": {
                                  "type": "string"
                                },
                                "description": "DNS servers"
                              }
                            }
                          },
                          "uplink": {
                            "type": "object",
                            "properties": {
                              "type": {
                                "type": "string",
                                "enum": ["wire", "wireless"],
                                "description": "Uplink connection type"
                              },
                              "up": {
                                "type": "boolean",
                                "description": "Whether uplink is up"
                              },
                              "full_duplex": {
                                "type": "boolean",
                                "description": "Whether uplink is full duplex"
                              },
                              "speed": {
                                "type": "integer",
                                "description": "Link speed in Mbps"
                              },
                              "mac": {
                                "type": "string",
                                "description": "MAC address of uplink device"
                              }
                            }
                          },
                          "downlink_table": {
                            "type": "array",
                            "description": "List of devices connected downstream",
                            "items": {
                              "type": "object",
                              "properties": {
                                "mac": {
                                  "type": "string",
                                  "description": "Device MAC address"
                                },
                                "type": {
                                  "type": "string",
                                  "description": "Device type"
                                },
                                "port": {
                                  "type": "integer",
                                  "description": "Connected port number"
                                }
                              }
                            }
                          },
                          "port_table": {
                            "type": "array",
                            "description": "List of ports (switches only)",
                            "items": {
                              "type": "object",
                              "properties": {
                                "port_idx": {
                                  "type": "integer",
                                  "description": "Port index"
                                },
                                "name": {
                                  "type": "string",
                                  "description": "Port name"
                                },
                                "up": {
                                  "type": "boolean",
                                  "description": "Whether port is up"
                                },
                                "enabled": {
                                  "type": "boolean",
                                  "description": "Whether port is enabled"
                                },
                                "poe_enabled": {
                                  "type": "boolean",
                                  "description": "Whether PoE is enabled"
                                },
                                "poe_mode": {
                                  "type": "string",
                                  "description": "PoE mode",
                                  "enum": ["auto", "24v", "passthrough", "off"]
                                },
                                "poe_power": {
                                  "type": "number",
                                  "description": "PoE power draw in watts"
                                },
                                "speed": {
                                  "type": "integer",
                                  "description": "Port speed in Mbps"
                                },
                                "full_duplex": {
                                  "type": "boolean",
                                  "description": "Whether port is full duplex"
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "access_point": {
                    "summary": "Access Point Example",
                    "value": {
                      "data": [
                        {
                          "mac": "f0:9f:c2:00:00:00",
                          "ip": "192.168.1.6",
                          "model": "U6-Lite",
                          "model_in_lts": false,
                          "model_in_eol": false,
                          "type": "uap",
                          "version": "7.4.162",
                          "adopted": true,
                          "status": "online",
                          "uptime": 1234567,
                          "last_seen": 1705609846,
                          "upgradable": false,
                          "provisioned": true,
                          "satisfaction": 98,
                          "satisfaction_reason": 0,
                          "sys_stats": {
                            "loadavg_1": 0.15,
                            "loadavg_5": 0.10,
                            "loadavg_15": 0.05,
                            "mem_total": 536870912,
                            "mem_used": 128000000,
                            "cpu": 5.2
                          },
                          "config_network": {
                            "type": "dhcp",
                            "ip": "192.168.1.6",
                            "netmask": "255.255.255.0",
                            "gateway": "192.168.1.1",
                            "dns": ["192.168.1.1", "1.1.1.1"]
                          },
                          "uplink": {
                            "type": "wire",
                            "up": true,
                            "full_duplex": true,
                            "speed": 1000,
                            "mac": "74:83:c2:00:00:00"
                          },
                          "name": "Office AP",
                          "serial": "FCDE12345678",
                          "stat": {
                            "bytes": 1234567890,
                            "rx_bytes": 987654321,
                            "tx_bytes": 246913578,
                            "rx_rate": 1024000,
                            "tx_rate": 512000,
                            "wifi_tx_attempts": 1000000,
                            "wifi_tx_drops": 1000
                          }
                        }
                      ],
                      "meta": {
                        "rc": "ok"
                      }
                    }
                  },
                  "switch": {
                    "summary": "Switch Example",
                    "value": {
                      "data": [
                        {
                          "mac": "74:83:c2:00:00:00",
                          "ip": "192.168.1.7",
                          "model": "USW-24-PoE",
